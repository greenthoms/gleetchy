{
  "presets": [["env", {
    "modules": false,
    "useBuiltIns": true,
    "targets": {
      "browsers": "> 1%, Last 2 versions",
      "uglify": true
    }
  }]],
  "plugins": [
    "inferno",
    "transform-object-rest-spread",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
